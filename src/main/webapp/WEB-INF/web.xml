<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>pack</servlet-name>
        <servlet-class>com.naturalmotion.servlet.PackServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>car</servlet-name>
        <servlet-class>com.naturalmotion.servlet.CarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>com.naturalmotion.servlet.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetFile</servlet-name>
        <servlet-class>com.naturalmotion.servlet.GetFile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pack</servlet-name>
        <url-pattern>/pack</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetFile</servlet-name>
        <url-pattern>/GetFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>car</servlet-name>
        <url-pattern>/car</url-pattern>
    </servlet-mapping>

</web-app>
